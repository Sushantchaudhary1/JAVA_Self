package Chaudhary;

public class Fibo {
    static int fibonacci(int n) {
        if (n == 0) {
            return n;
        }
        if (n == 1 || n == 2) {
            return 1;
        } else return fibonacci(n - 1) + fibonacci(n - 2);
    }

    public static void main(String[] args) {
        int x = 10;
        System.out.println("The fibonacci of x is");
        for (int i = 0; i < x; i++) {
            System.out.print(fibonacci(i) + " ");

        }
    }
}

